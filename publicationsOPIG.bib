keywords = {Amino Acid Sequence,Binding Sites,Computer Graphics,Cytokine,Humans,Models,Molecular,Molecular Sequence Data,Molecular Structure,Neoplasm Proteins,Protein Conformation,Proto-Oncogene Proteins,Receptors,Sequence Alignment,Thrombopoietin},
