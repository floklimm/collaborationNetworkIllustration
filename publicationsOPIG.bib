keywords = {Amino Acid,Amino Acid Sequence,Database Management Systems,Databases,Factual,Molecular Sequence Data,Protein Conformation,Sequence Homology},
month = {nov},
number = {Pt 6 Pt 1},
pages = {1168--1177},
